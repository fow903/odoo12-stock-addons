<openerp>
  <data>
   <record model="ir.ui.view" id="sale_view_form_qty_available">
      <field name="name">sale order item no</field>
      <field name="model">sale.order</field>
      <field name="inherit_id" ref="sale.view_order_form"/>
      <field name="arch" type="xml">
         <xpath expr="//sheet/notebook/page/field[@name='order_line']/tree/field[@name='product_id']" position="after">

           <field name="qty_available" readonly='1'  
           />
        </xpath>


      </field>
    </record>


  </data>
</openerp>
